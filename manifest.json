{
    "manifest_version": 3,
    "name": "YT Skip",
    "version": "1.0",
    "icons": {
      "32": "lynx32.png",
      "64": "lynx64.png",
      "128": "lynx128.png"
    },
    "action": {
      "default_icon": "lynx64.png"
    },
    "description": "Skip skippable ads on Youtube.",
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  