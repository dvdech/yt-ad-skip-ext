{
    "manifest_version": 3,
    "name": "YT Skip",
    "version": "1.2",
    "icons": {
      "32": "yt-ad-skip-icon32.png",
      "64": "yt-ad-skip-icon64.png",
      "128": "yt-ad-skip-icon128.png"
    },
    "action": {
      "default_icon": "yt-ad-skip-icon128.png"
    },
    "description": "Skip skippable ads on Youtube.",
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  